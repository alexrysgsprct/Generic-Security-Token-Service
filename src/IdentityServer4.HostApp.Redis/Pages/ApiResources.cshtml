@page "{apiResourceId?}"
@model ApiResourcesModel
@{
    ViewData["Title"] = "API Resources";
}

 
<div class="container-fluid">
    <div class="row">
        <div class="sidebar3 col-sm-3 ">
            @await  Component.InvokeAsync("Apis")
            <vc:apis></vc:apis>
        </div>
        <div class="main2 col-sm-8 ">
            @await Component.InvokeAsync("Api", new { id = Model.ClientId })
        </div>
    </div>
</div>

@section StyleHead
    {
    <link rel="stylesheet" href="~/css/sidebar.css" />
 

}
